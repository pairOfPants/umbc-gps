{
    "compilerOptions": {
        "target": "es2015",
    "module": "es2015",
        "lib": ["es2015", "dom", "es2016", "es2017", "es2018", "es2019", "es2020"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./build"
    },
    "include": ["./*.ts"],
    "exclude": ["node_modules", "old"]
}
{
    "compilerOptions": {
        "target": "es2015",
    "module": "commonjs",
        "lib": ["es2015", "dom", "es2016", "es2017", "es2018", "es2019", "es2020"],
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist"
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules", "old"]
}